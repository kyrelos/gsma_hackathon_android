<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.princek.gsmahakathon"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tigoafrica"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.princek.gsmahakathon.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.princek.gsmahakathon.LandingActivity"
            android:label="@string/title_activity_landing"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.princek.gsmahakathon.ConsumerActivity"
            android:label="@string/title_activity_consumer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.princek.gsmahakathon.AgentActivity"
            android:label="@string/title_activity_agent"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.princek.gsmahakathon.ConsumerWithdrawActivity"
            android:label="@string/title_activity_consumer_withdraw"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.princek.gsmahakathon.ConsumerSendActivity"
            android:label="@string/title_activity_consumer_send"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.princek.gsmahakathon.ConsumerStatementActivity"
            android:label="@string/title_activity_consumer_statement"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.princek.gsmahakathon.ThankYouActivity" />
        <activity
            android:name="com.example.princek.gsmahakathon.DepositActivity"
            android:label="@string/title_activity_deposit"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>